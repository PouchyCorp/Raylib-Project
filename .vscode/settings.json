{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.o": true,
		"**/*.exe": true,
		"**/*.gitattributes": true,
		"**/*.gitignore": true
	},
	"files.autoSave": "afterDelay",
	"files.associations": {
		"*.tcc": "cpp",
		"ostream": "cpp",
		"*.m": "cpp"
	}
}